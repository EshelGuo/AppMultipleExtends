<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#fff"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.eshel.demo.MainActivity">
    <TextView android:id="@+id/show" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <Button android:id="@+id/select" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <com.eshel.demo.MVideoView
        android:id="@+id/videoview"
        android:visibility="gone"
        android:layout_width="match_parent" android:layout_height="match_parent"/>

</RelativeLayout>
